{
  "name": "@pinsel/renderer",
  "description": "The renderers by pinsel",
  "version": "0.0.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Luis Q. Haitzer"
  },
  "repository": {
    "url": "https://github.com/knallbumm/pinsel"
  },
  "homepage": "https://github.com/knallbumm/pinsel#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "rollup-build": "rimraf dist/ && rollup --config rollup.config.ts --configPlugin typescript",
    "lint": "eslint . --ext .ts",
    "lint:files": "eslint --ext .ts --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit -p .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@pinsel/core": "workspace:*",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitest/coverage-c8": "^0.26.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "vitest": "^0.26.3"
  }
}
